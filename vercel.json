{
  "version": 2,
  "trailingSlash": false,
  "redirects": [
    { "source": "/twitter", "destination": "https://twitter.com/CryptoValorex" },
    {
      "source": "/facebook",
      "destination": "https://www.facebook.com/ValorExCrypto/"
    },
    {
      "source": "/instagram",
      "destination": "https://www.instagram.com/Valorexcrypto/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
